        <xpath expr="." position="inside">
            <script type="text/javascript" src="/pos_backend_delivery/static/src/js/pos_delivery.js"></script>
        </xpath>
    </template>
    <template id="assets_backend" name="back delivery " inherit_id="web.assets_backend">
